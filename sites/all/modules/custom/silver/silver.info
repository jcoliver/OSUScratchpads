package = "Import"
core = 7.x
dependencies[] = libraries
dependencies[] = feeds
dependencies[] = taxonomy
dependencies[] = feeds_tamper
dependencies[] = feeds_xls
dependencies[] = feeds_file_processor
dependencies[] = feeds_term_reference
dependencies[] = auto_entitylabel
dependencies[] = media_feeds
files[] = includes/FeedsNodeProcessorValidate.inc
features[ctools][] = "feeds:feeds_importer_default:1"
l10n server = localize.scratchpads.eu
l10n url = http://localize.scratchpads.eu/files/l10n_packager/l10n_server.xml
project = silver